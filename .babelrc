{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": ["last 2 versions", "safari >= 7"]
        },
        "modules": false
      }
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        // "corejs": 0,
        // "helpers": true,
        // "regenerator": true,
        // "useESModules": true,
        "asyncGenerators": false,
        "generators": false,
        "async": false
      }
    ]
  ]
}
